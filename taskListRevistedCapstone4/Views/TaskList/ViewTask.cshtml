
@{
    ViewData["Title"] = "ViewTask";
}

<h1>ViewTask</h1>
@model IEnumerable<UserTask>
<div class="row">
    @foreach (var task in Model)
    {
        <div class="col-md-6">
            <h5>@task.Task</h5>
            <h6> @task.DueDate</h6>
            <p>complete: @task.Complete</p>
            <a href="/TaskList/DeleteTask?Id=@task.Id"> <input type="submit" value="Delete Task" /> </a>
        </div>
    }
</div>
@Html.ActionLink("Add Task", "AddTask")